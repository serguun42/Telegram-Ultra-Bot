{
  "name": "animeultrabot",
  "version": "3.8.2",
  "description": "Telegram bot based on Social-Picker-API. Includes features from Anime-Twitter-Picker Bot, Anime Spoiler Bot and Anime Welcomer Bot",
  "main": "animeultrabot.js",
  "type": "module",
  "scripts": {
    "production": "pm2 start config/pm2.production.json",
    "dev": "nodemon animeultrabot.js --config config/nodemon.dev.json",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serguun42/Anime-Ultra-Bot.git"
  },
  "keywords": [
    "telegram",
    "telegram-bot",
    "telegraf",
    "twitter",
    "instagram",
    "pixiv",
    "ugoira",
    "reddit",
    "youtube-dl",
    "yt-dlp",
    "osnova",
    "dtf",
    "tumblr",
    "joyreactor",
    "coub"
  ],
  "author": "serguun42",
  "license": "BSL-1.0 License",
  "dependencies": {
    "khaleesi-js": "github:serguun42/Khaleesi-JS",
    "node-fetch": "^3.3.1",
    "pm2": "^5.3.0",
    "telegraf": "^4.12.2"
  },
  "devDependencies": {
    "@types/node": "^18.16.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  }
}
